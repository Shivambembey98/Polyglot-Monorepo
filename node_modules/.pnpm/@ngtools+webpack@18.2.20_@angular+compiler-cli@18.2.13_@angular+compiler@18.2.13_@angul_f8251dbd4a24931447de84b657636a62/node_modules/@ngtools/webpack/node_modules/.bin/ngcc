#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/bundles/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/bundles/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules:/home/ec2-user/Polyglot-Monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/bundles/ngcc/index.js" "$@"
else
  exec node  "$basedir/../../../../../../@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8._d731cc84e2c35337dd976ce35fabd974/node_modules/@angular/compiler-cli/bundles/ngcc/index.js" "$@"
