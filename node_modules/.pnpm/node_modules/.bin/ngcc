#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/compiler-cli/bundles/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/compiler-cli/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/compiler-cli/bundles/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/compiler-cli/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules/@angular/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_typescript@5.5.4/node_modules:/home/runner/work/Polyglot-Monorepo/Polyglot-Monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@angular/compiler-cli/bundles/ngcc/index.js" "$@"
else
  exec node  "$basedir/../@angular/compiler-cli/bundles/ngcc/index.js" "$@"
fi
